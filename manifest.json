{
    "manifest_version": 3,
    "name": "SynCryptHash",
    "version": "1.1",
    "description": "Хэшер от любимого разработчика!",
    "author": "Kamishiro Iyamoto",
    "icons": {
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    },
    "action": {
        "default_title": "SynCryptHash. Хэшер от любимого разработчика!",
		"default_icon": "128.png",
		"default_popup": "popup.html",
        "content_scripts": [
            {
                "matches": [ "<all_urls>" ],
                "css": ["style.css"],
                "js": [ "jsbn.js", "jsbn2.js", "rng.js", "prng4.js", "rsa.js", "rsa2.js", "rsa3.js" ]
            }
        ]
	}
 }